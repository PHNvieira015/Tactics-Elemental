MonoBehaviour:
  m_ObjectHideFlags: 0
  m_CorrespondingSourceObject: {fileID: 0}
  m_PrefabInstance: {fileID: 0}
  m_PrefabAsset: {fileID: 0}
  m_GameObject: {fileID: 1918112072}
  m_Enabled: 1
  m_EditorHideFlags: 0
  m_Script: {fileID: 11500000, guid: c73aa27fb016f6d4387be721e2660213, type: 3}
  m_Name: 
  m_EditorClassIdentifier: 
  characters:
  - name: Pai
  - name: '{ProtagonistName}'
  - name: Zappi
  - name: Pai e {ProtagonistName}
  dialogue:
    messages:
    - characterID: 0
      message: "Filho? Para qu\xEA \xE9 tudo isso?"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 1
      message: Eu estou indo procurar uma magia para salvar o mestre. Alguma das
        outras vilas devem saber fazer isso.
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 0
      message: "O qu\xEA? Voc\xEA n\xE3o pode fazer isso, \xE9 muito perigoso! Quem
        sabe o perigo que voc\xEA vai encontrar!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 1
      message: Mas eu devo tentar, tenho que salvar o mestre! Por isso que eu estou
        treinando a meses!
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 0
      message: "N\xE3o seja tolo! Acontecer\xE1 a mesma coisa do que com a sua m\xE3e!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 3
    - characterID: 0
      message: "Me entenda, filho. Mesmo que voc\xEA chegasse at\xE9 uma vila, elas
        n\xE3o gostam uma das outras, v\xE3o te mandar prender assim que te verem.
        Se pelo menos voc\xEA tamb\xE9m soubesse magia, s\xF3 com essa sua arma n\xE3o
        vai poder se defender direito."
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 2
      message: E se eu for com ele, pai?
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 3
      message: "O qu\xEA?"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 2
      message: "Eu tamb\xE9m fui treinada pelo mestre! Quero ajudar tamb\xE9m!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 1
      message: "N\xE3o, n\xE3o, voc\xEA \xE9 muito jovem!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 0
      message: "Isso, isso! N\xE3o, pera\xED, voc\xEA tamb\xE9m \xE9, {ProtagonistName}!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 2
      message: "A\xED, pelo amor do Deus. Irm\xE3o, voc\xEA deveria deixar que eu
        v\xE1 contigo. Eu tamb\xE9m recebi o treinamento do mestre com uma arma,
        e at\xE9 sei utilizar um elemento. Se formos n\xF3s dois, o papai n\xE3o
        ter\xE1 motivo nenhum para n\xE3o deixar."
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 0
      message: "N\xE3o, pera\xED, \xE9 claro que eu tenho!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 1
      message: "Voc\xEA tem um ponto, Zappi. T\xE1 bom, eu deixarei voc\xEA me acompanhar
        para te proteger!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 0
      message: "Eu ainda n\xE3o aprovei isto!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 1
      message: "Por favor, pai! Se estamos os dois juntos n\xE3o tem como acontecer
        nada, os dois nos protegermos um ao outro. Algu\xE9m tem que salvar o mestre!
        E seu visitar outras vilas\u2026 Talvez finalmente aprenda magia!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 2
    - characterID: 0
      message: "Esse sempre foi o seu sonho, n\xE9? Eu gostaria que algum dos dois
        tivesse tido tanta paix\xE3o em aprender sobre a minha profiss\xE3o do que
        sobre magia, mas acho que voc\xEAs v\xE3o mesmo que eu diga que n\xE3o..."
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 2
    - characterID: 0
      message: "V\xE1, filho, aprenda e se esforce muito. Transforme toda essa paix\xE3o
        em dedica\xE7\xE3o! E tenham muito cuidado, meus filhos."
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 2
      message: Eu vou buscar minhas coisas!
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
    - characterID: 1
      message: "N\xE3o demore, ein!"
      moveCharacter: {fileID: 0}
      startPosition: {x: 0, y: 0}
      endPosition: {x: 0, y: 0}
      waitTime: 0
  TriggerOrder: 7
